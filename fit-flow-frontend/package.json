{
    "name": "fit-flow",
    "version": "0.0.0",
    "private": true,
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "preview": "vite preview",
        "build-only": "vite build",
        "build:dev": "vite build --mode development",
        "build:prod": "vite build --mode production",
        "type-check": "vue-tsc --build",
        "lint:oxlint": "oxlint . --fix -D correctness --ignore-path .gitignore",
        "lint:eslint": "eslint . --fix",
        "lint": "run-s lint:*",
        "format": "prettier --write src/",
        "deploy:dev": "bun run build:dev && rsync -avz --delete dist/ root@147.45.154.109:/var/www/theomnia.ru/dist",
        "deploy:prod": "bun run build:prod && rsync -avz --delete dist/ root@147.45.154.109:/var/www/theomnia.ru/dist"
    },
    "dependencies": {
        "axios": "^1.8.4",
        "sass": "^1.86.3",
        "scss": "^0.2.4",
        "vant": "^4.9.18",
        "vue": "^3.5.13",
        "vue-router": "^4.5.0",
        "vuex": "^4.1.0"
    },
    "devDependencies": {
        "@tsconfig/node22": "^22.0.0",
        "@types/node": "^22.13.9",
        "@vant/auto-import-resolver": "^1.3.0",
        "@vitejs/plugin-vue": "^5.2.1",
        "@vitejs/plugin-vue-jsx": "^4.1.1",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.5.0",
        "@vue/tsconfig": "^0.7.0",
        "eslint": "^9.21.0",
        "eslint-plugin-oxlint": "^0.15.13",
        "eslint-plugin-vue": "~10.0.0",
        "jiti": "^2.4.2",
        "npm-run-all2": "^7.0.2",
        "oxlint": "^0.15.13",
        "prettier": "3.5.3",
        "typescript": "~5.8.0",
        "unplugin-auto-import": "^19.1.2",
        "unplugin-vue-components": "^28.4.1",
        "vite": "^6.2.1",
        "vite-plugin-vue-devtools": "^7.7.2",
        "vue-tsc": "^2.2.8"
    }
